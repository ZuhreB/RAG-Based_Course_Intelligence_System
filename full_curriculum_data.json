    {
        "meta": {
            "department": "Software Engineering",
            "semester": "1. Year Fall Semester",
            "code": "MATH 153",
            "name": "Calculus I",
            "ects": "6",
            "link": "https://ects.ieu.edu.tr/new/syllabus.php?section=se.cs.ieu.edu.tr&course_code=MATH 153&currType=before_2025"
        },
        "details": {
            "theory_hours": "2",
            "lab_hours": "2",
            "local_credit": "3",
            "objectives": "This course aims to built fundamentals of calculus and its applications for engineers",
            "description": "Calculus I provides important tools in understanding functions of one variable and has led to the development of new areas of mathematics.",
            "learning_outcomes": [
                "find limits of functions.",
                "investigate continuity of functions.",
                "compute derivatives of explicit and implicit functions.",
                "solve related rates problems.",
                "classify critical points of functions.",
                "sketch graphs of functions.",
                "solve extreme value problems.",
                "compute areas of plane regions."
            ],
            "weekly_schedule": [
                {
                    "week": "1",
                    "topic": "Graphs of quadratic functions, Polynomials and rational functions, the trigonometric functions, examples of velocity, growth rate and area"
                },
                {
                    "week": "2",
                    "topic": "Limits of Functions, limits at infinity and infinite limits"
                },
                {
                    "week": "3",
                    "topic": "Continuity, tangent lines and their slopes"
                },
                {
                    "week": "4",
                    "topic": "The derivative, differentiation rules, the chain rule, derivatives of trigonometric functions"
                },
                {
                    "week": "5",
                    "topic": "Higher-order derivatives, the mean value theorem"
                },
                {
                    "week": "6",
                    "topic": "Implicit differentiation, inverse functions, Exponential and logarithmic functions"
                },
                {
                    "week": "7",
                    "topic": "The natural logarithm and exponential. The inverse trigonometric functions"
                },
                {
                    "week": "8",
                    "topic": "Midterm"
                },
                {
                    "week": "9",
                    "topic": "Related rates, indeterminate forms"
                },
                {
                    "week": "10",
                    "topic": "Extreme values, concavity and inflections"
                },
                {
                    "week": "11",
                    "topic": "Sketching the graph of a function, extreme value problems"
                },
                {
                    "week": "12",
                    "topic": "Extreme value problems properties of the definite integral.The fundamental theorem of calculus"
                },
                {
                    "week": "13",
                    "topic": "The method of substitution"
                },
                {
                    "week": "14",
                    "topic": "The method of substitution, areas of plane regions"
                },
                {
                    "week": "15",
                    "topic": "Semester review"
                },
                {
                    "week": "16",
                    "topic": "Final exam"
                }
            ]
        }
    },
