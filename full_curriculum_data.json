    {
        "meta": {
            "department": "Software Engineering",
            "semester": "1. Year Fall Semester",
            "code": "PHYS 100",
            "name": "General Physics I",
            "ects": "6",
            "link": "https://ects.ieu.edu.tr/new/syllabus.php?section=se.cs.ieu.edu.tr&course_code=PHYS 100&currType=before_2025"
        },
        "details": {
            "theory_hours": "2",
            "lab_hours": "2",
            "local_credit": "3",
            "objectives": "The purpose of this course is to teach the fundamental laws of mechanics and introduce students to the basic applications of these laws.",
            "description": "In this course, we will discuss the subjects of motion along a straight line, motion in two and three dimensions, Newton’s laws, work and kinetic energy, potential energy and conservation of energy, momentum, collisions, dynamics of rotations, gravitation and periodic motion.",
            "learning_outcomes": [
                "determine the motion of objects in one, two and three dimensions using the laws of kinematics.",
                "use Newton’s laws to solve mechanics problems.",
                "calculate the kinetic and potential energies of a given mechanical system.",
                "analyze the dynamics of collisions and explosions using the concept of momentum.",
                "discuss the rotations of rigid bodies and their dynamics.",
                "describe the dynamics of objects in circular and periodic motion.",
                "use experimental setups to collect and analyze data."
            ],
            "weekly_schedule": [
                {
                    "week": "1",
                    "topic": "Introduction, measurement, estimating"
                },
                {
                    "week": "2",
                    "topic": "Vectors"
                },
                {
                    "week": "3",
                    "topic": "Kinematics in one dimension"
                },
                {
                    "week": "4",
                    "topic": "Kinematics in two and three dimensions"
                },
                {
                    "week": "5",
                    "topic": "Newton’s laws"
                },
                {
                    "week": "6",
                    "topic": "Applications of Newton’s laws"
                },
                {
                    "week": "7",
                    "topic": "Gravitations"
                },
                {
                    "week": "8",
                    "topic": "Midterm exam"
                },
                {
                    "week": "9",
                    "topic": "Work and Kinetic energy"
                },
                {
                    "week": "10",
                    "topic": "Potential energy and conservation of energy"
                },
                {
                    "week": "11",
                    "topic": "Linear momentum conservation"
                },
                {
                    "week": "12",
                    "topic": "Linear momentum and collisions"
                },
                {
                    "week": "13",
                    "topic": "Rotational motion"
                },
                {
                    "week": "14",
                    "topic": "Angular momentum"
                },
                {
                    "week": "15",
                    "topic": "Semester review"
                },
                {
                    "week": "16",
                    "topic": "Final exam"
                }
